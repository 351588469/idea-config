<templateSet group="userTemplate">
  <template name="comt" value="/**&#10; * @package: $package$&#10; * @description: $description$&#10; * @author: $user$&#10; * @date: $date$&#10; **/" description="standred comment" toReformat="false" toShortenFQNames="true">
    <variable name="package" expression="currentPackage()" defaultValue="" alwaysStopAt="false" />
    <variable name="description" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="user" expression="lowercaseAndDash(&quot;zhangxiancheng&quot;)" defaultValue="zxc" alwaysStopAt="false" />
    <variable name="date" expression="date(&quot;yyyy-MM-dd hh:mm&quot;)" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="pi" value="private Integer $END$" description="private Integer" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="ps" value="private String $END$" description="private String" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="pd" value="private Double $END$" description="private Double" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="pb" value="private Boolean $END$" description="private Boolean" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>